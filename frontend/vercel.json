{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/.*",
      "dest": "/index.html"
    }
  ],
  "env": {
    "REACT_APP_API_URL": "https://anomaly-detection-backend.onrender.com",
    "REACT_APP_WS_URL": "wss://anomaly-detection-backend.onrender.com"
  },
  "build": {
    "env": {
      "REACT_APP_API_URL": "https://anomaly-detection-backend.onrender.com",
      "REACT_APP_WS_URL": "wss://anomaly-detection-backend.onrender.com"
    }
  }
}
